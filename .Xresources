! Enable the extended coloring options
rofi.modi: combi,window,run,ssh
rofi.combi-modi:window,run,ssh
rofi.show:combi
rofi.color-enabled: true
!                  bg             border   separator
rofi.color-window: argb:af303030, argb:00000000,  900000,
!                  bg             fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: argb:00000000, #FFFFFF, 000000, 000000,   #f8ae19
rofi.color-active: argb:00000000, #FFFFFF, argb:00000000,   argb:00000000, #ff9944
rofi.color-urgent: #2f343f, #2f343f, #2f343f, #2f343f, #2f343f

rofi.separator-style: none
rofi.sidebar-mode: true
rofi.bw: 1
rofi.columns: 1
rofi.padding: 20

rofi.yoffset: 0
rofi.fake-transparency: false
rofi.location: 0
rofi.width: 1000
rofi.font:Awa Sans Mono 15
rofi.lines: 15
rofi.fixed-num-lines: true
